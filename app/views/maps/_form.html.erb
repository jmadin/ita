<%= form_for(@map) do |f| %>
  <%= render 'shared/error_messages', object: f.object %>

  <div class="form-group">
    <%= f.label :map_name %><br>
    <%= f.text_field :map_name, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :map_code %><br>
    <%= f.text_field :map_code, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :latitude %><br>
    <%= f.text_field :latitude, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :pixel_size %><br>
    <%= f.text_field :pixel_size, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :longitude %><br>
    <%= f.text_field :longitude, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :map_description %><br>
    <%= f.text_area :map_description, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :map_date %><br>
    <%= f.date_select :map_date, class: "form-control" %>
  </div>

  <%= f.submit "Submit", class: "btn btn-large btn-primary" %>
<% end %>
